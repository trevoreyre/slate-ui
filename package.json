{
  "name": "@slate-ui/core",
  "version": "2.0.1",
  "description": "Web component library",
  "private": false,
  "keywords": [
    "web components",
    "component",
    "library",
    "UI"
  ],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/slate-ui",
  "bugs": "https://github.com/trevoreyre/slate-ui/issues",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/slate-ui-stencil/slate-ui-stencil.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "install:package-lock": "npm install --package-lock-only --workspaces false",
    "start": "concurrently \"npm:start:*\"",
    "start:storybook": "storybook dev -p 9005",
    "start:stencil": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "prebuild": "rimraf dist loader storybook-static",
    "build": "npm run build:stencil && npm run build:storybook",
    "build:stencil": "stencil build --docs",
    "build:storybook": "storybook build",
    "build:typography": "node scripts/build-theme-typography.js"
  },
  "dependencies": {
    "@stencil/core": "^4.7.0"
  },
  "devDependencies": {
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/metrics": "^0.1.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/html": "^7.6.7",
    "@storybook/html-vite": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "just-extend": "^6.0.1",
    "outdent": "^0.8.0",
    "puppeteer": "21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.6.7"
  }
}
