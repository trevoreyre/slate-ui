{
  "name": "@slate-ui/core",
  "version": "1.1.0",
  "description": "Web component library",
  "private": false,
  "keywords": [
    "web components",
    "component",
    "library",
    "UI"
  ],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/slate-ui",
  "bugs": "https://github.com/trevoreyre/slate-ui/issues",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "customElements": "custom-elements.json",
  "scripts": {
    "install:package-lock": "npm install --package-lock-only --workspaces false",
    "start": "storybook dev -p 9002",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint '**/*.js'",
    "lint:lit-analyzer": "lit-analyzer src",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "build": "npm run build:custom-elements-manifest && npm run build:typography && npm run build:storybook",
    "build:custom-elements-manifest": "custom-elements-manifest analyze",
    "build:storybook": "storybook build",
    "build:typography": "node scripts/build-theme-typography.js"
  },
  "devDependencies": {
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/metrics": "^0.1.0",
    "@custom-elements-manifest/analyzer": "^0.5.3",
    "@storybook/addon-essentials": "^7.0.13",
    "@storybook/addon-links": "^7.0.13",
    "@storybook/blocks": "^7.0.13",
    "@storybook/web-components": "^7.0.13",
    "@storybook/web-components-vite": "^7.0.13",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.6.12",
    "just-extend": "^6.0.1",
    "lit": "^2.8.0",
    "lit-analyzer": "^1.1.10",
    "outdent": "^0.8.0",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.0.13"
  }
}
